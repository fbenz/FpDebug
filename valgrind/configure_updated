$as_echo "2.6 family" >&6; }

$as_echo "#define GLIBC_2_6 1" >>confdefs.h

	DEFAULT_SUPP="glibc-2.6.supp ${DEFAULT_SUPP}"
	DEFAULT_SUPP="glibc-2.34567-NPTL-helgrind.supp ${DEFAULT_SUPP}"
